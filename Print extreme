printing left the rightmost elements 

both odd and even case handeled 

#include<iostream>
using namespace std;

void printExtreme(int arr[], int size){
  int left = 0;
  int right = size-1;
  while(left<=right){
    if(left == right){
      cout<<arr[left]<<" ";
    }else{
    cout<<arr[left]<<" ";
    cout<<arr[right]<<" ";
    }
    left++;
    right--;
  }
}

int main(){
  int arr[] = {23, 45, 12, 43, 29, 76, 69, 89,34};
   int size = sizeof(arr)/sizeof(arr[0]);
   printExtreme(arr, size);
  return 0 ;
}
